{
  "name": "gm-export-pdfs-core",
  "type": "module",
  "version": "1.0.2",
  "description": "The Core of VuePress and VitePress exports PDF plugin",
  "author": "garminzjm",
  "license": "MIT",
  "homepage": "https://github.com/garmin-z/gm-export-pdf-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garmin-z/gm-export-pdf-core.git"
  },
  "bugs": {
    "url": "https://github.com/garmin-z/gm-export-pdf-core/issues"
  },
  "keywords": [
    "vuepress-plugin",
    "vuepress",
    "plugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "release": "bumpp -r && pnpm -r publish --no-git-checks",
    "build": "unbuild",
    "dev": "esno ./src/index.ts"
  },
  "dependencies": {
    "@condorhero/merge-pdfs": "^0.0.7",
    "bundle-require": "^4.1.0",
    "cac": "^6.7.14",
    "envinfo": "^7.13.0",
    "fs-extra": "^11.2.0",
    "html-export-pdf-cli": "^1.1.5",
    "multimatch": "^7.0.0",
    "ora": "^8.0.1",
    "pdfjs": "^2.5.3",
    "picocolors": "^1.0.0",
    "semver": "^7.6.2",
    "unbuild": "^2.0.0"
  },
  "devDependencies": {
    "@types/envinfo": "^7.8.3",
    "@types/fs-extra": "^11.0.4",
    "@types/semver": "^7.5.8",
    "bumpp": "^9.8.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}